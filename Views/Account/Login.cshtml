@model Accounts

<h4>@TempData["LoggedIn"]</h4>
<form asp-controller="Account" asp-action="Login" method="post">
	<div class="p-3 mt-4">
		<div class="row pb-2 text-center">
			<h1 class="text-secondary">Log In</h1>
			<hr />
			<div asp-validation-summary="All" class="text-danger"></div>
		</div>
		<div class="mb-3 text-center">
			<h3><b><label asp-for="Title"></label></b></h3>
			<input asp-for="Title" class="form-control m-auto" style="width: 400px;"/>
			<span asp-validation-for="Title" class="text-danger"></span>
			<hr/>
		</div>
		<div class="text-center"><button type="submit" class="btn btn-primary m-auto" fdprocessedid="vccu1">Log In</button></div>
	</div>
</form>