@model Character

@if (Model != null)
{
<div class="mt-3">
	<ul class="nav nav-tabs justify-content-center" role="tablist">
		<li class="nav-item active">
			<button data-bs-toggle="tab" data-bs-target="#details" role="tab" class="nav-link active">Details</button>
		</li>
		<li class="nav-item">
			<button data-bs-toggle="tab" data-bs-target="#attributes" role="tab" class="nav-link">Attributes</button>
		</li>
		<li class="nav-item">
			<button data-bs-toggle="tab" data-bs-target="#skills" role="tab" class="nav-link">Skills & Qualities</button>
		</li>
		<li class="nav-item">
			<button data-bs-toggle="tab" data-bs-target="#sheet" role="tab" class="nav-link">Interactive Character Sheet</button>
		</li>
	</ul>
	<div class="tab-content">
		<div class="tab-pane active" id="details" role="tabpanel">
			<partial name="EditDetailsPartial.cshtml" for="@Model"/>
		</div>
		<div class="tab-pane" id="attributes" role="tabpanel">
			<partial name="EditAttributesPartial.cshtml" for="@Model"/>
		</div>
		<div class="tab-pane" id="skills" role="tabpanel">
			<partial name="EditSkillsQualitiesPartial.cshtml" for="@Model"/>
		</div>
		<div class="tab-pane" id="sheet" role="tabpanel">
			<partial name="CharacterSheetPartial.cshtml" for="@Model"/>
		</div>
	</div>
</div>
}
else
{

}